URxvt.transparent: false
URxvt.shading: 0
URxvt.font: xft:xos4 Terminus:pixelsize=15:minspace=False, xft:Inconsolata Nerd Font Mono

URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 14
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.depth: 24
URxvt.iso14755: false

URxvt.keysym.Shift-Control-V:	eval:paste_clipboard
URxvt.keysym.Shift-Control-C:	eval:selection_to_clipboard
URxvt.keysym.Shift-Control-K:	command:\033]720;5\007
URxvt.keysym.Shift-Control-J:	command:\033]721;5\007
URxvt.keysym.Control-Down:		resize-font:smaller
URxvt.keysym.Control-Up:		resize-font:bigger

rofi.font: Terminus, bold 15
rofi.lines: 10
rofi.terminal: urxvt

Xcursor.theme: Simp1e 
Xcursor.size:  22

